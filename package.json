{
  "name": "cesium-draw",
  "version": "2.3.2",
  "description": "add a mark,polyline,polygon base on Cesium",
  "private": false,
  "keyword": "Cesium Polygon Polyline mark interactive",
  "main": "./dist/cesium-drawHandler.common.js",
  "files": [
    "dist",
    "src",
    "public",
    "*.js"
  ],
  "scripts": {
    "build": "vue-cli-service build --target lib --name cesium-drawHandler ./src/index.js",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve"
  },
  "dependencies": {
    "babel-plugin-component": "^1.1.1",
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "file-saver": "^2.0.2",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "shapefile": "^0.6.6",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "css-loader": "^0.28.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.10",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
